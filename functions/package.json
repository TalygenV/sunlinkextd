    "firebase-functions": "^6.3.2",
    "stripe": "^18.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.19",
